[user]
	name = bblake
	email = bblake@goempyrean.com
[push]
	default = simple
[color]
	branch = auto
	diff = true
	status = auto
	ui = true
[color "branch"]
	current = yellow reverse
	local = yellow
	remote = green
[color "diff"]
	meta = yellow bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = yellow
	changed = green
	untracked = cyan 
[core]
	editor = vim
	autocrlf = input
[rerere]
	enabled = true
	autoupdate = true
[alias]
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
	push-i = push --force-with-lease --progress origin develop qa
[help]
	autocorrect = 1
